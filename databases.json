            "descripGrp": {
                "mappings": [
                    {
                        "key": ["descrip", "$", "type"],
                        "value": ["descrip", "$text"]
                    }
                ]
            },
            "languageGrp": {
                "mappings": [
                    {
                        "key": "language",
                        "mappings": [
                            {
                                "key": "languageType",
                                "value": ["language", "$", "type"]
                            },
                            {
                                "key": "languageCode",
                                "value": ["language", "$", "lang"]
                            }
                        ]
                    }
                ]
            },
            "termGrp": {
                "mappings": [
                    {
                        "key": "terms",
                        "mappings": [
                            {
                                "key": "term",
                                "value": ["term"]
